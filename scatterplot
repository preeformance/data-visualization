    text-align: center; 
}
.dot {
  
}
</style> 

#JavaScript
fetch('https://raw.githubusercontent.com/freeCodeCamp/ProjectReferenceData/master/cyclist-data.json')
.then(dataset)
.then() 

svg.selectAll("circle")
  .data(dataset)
  .enter()
  .append("circle")
  .attr("class", "dot")
  .attr("cx", (d) => d[0] )
  .attr("cy", (d) => h-d[1])
  .attr("r", 5) 
